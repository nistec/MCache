<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
     <section name="SyncConfig" type="Nistec.Caching.Sync.SyncConfig, Nistec.Cache, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
    <section name="ConnectionSettings" type="Nistec.Data.Ado.ConnectionSettings, Nistec.Cache, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
  </configSections>
  
<SyncConfig>

  <SyncCache>

    <SyncItem
  EntityType="GenericEntity"
    ConnectionKey="Netcell_Docs"
    EntityName="accountEntity"
    MappingName="Accounts"
    SourceName="Accounts"
    SourceType="View"
    EntityKeys="AccountId"
  Columns="[AccountId],[AccountName],[Email],[Mobile],[City],[OwnerId],[LastUpdate]"
    SyncType="Interval"
    SyncTime="00:51:30" />


    <SyncItem
         EntityType="GenericEntity"
         ConnectionKey="Netcell_Docs"
         EntityName="accountEntityTable"
         MappingName="Accounts"
         SourceName="Accounts"
         SourceType="Table"
         EntityKeys="AccountId"
       EnableNoLock="true"
         SyncType="Interval"
         SyncTime="00:52:00" />


    <SyncItem
        EntityType="GenericEntity"
          ConnectionKey="Netcell_Docs"
          EntityName="Accounts_CategoryEntity"
          MappingName="Accounts_Category"
          SourceName="Accounts_Category"
          SourceType="Table"
          EntityKeys="AccountCategoryId"
          SyncType="Event"
          SyncTime="00:52:30" />

    <SyncItem
        EntityType="GenericEntity"
          ConnectionKey="Netcell_Docs"
          EntityName="Accounts_Type"
          MappingName="Accounts_Type"
          SourceName="Accounts_Type"
          SourceType="Table"
          EntityKeys="[AccTypeId],[AccTypeName],[AccTypeHe]"
          SyncType="Interval"
          SyncTime="00:24:30" />

  </SyncCache>

</SyncConfig>

  <ConnectionSettings>

    <connectionStrings>
      <add name="AdventureWorks" connectionString="Data Source=NISTEC-PC;Initial Catalog=AdventureWorks;Integrated Security=True;" providerName="System.Data.SqlClient" />
      <add name="EnterpriseDB" connectionString="Data Source=NISTEC-PC;Initial Catalog=EnterpriseDB;Integrated Security=True;" providerName="System.Data.SqlClient" />
    </connectionStrings>

  </ConnectionSettings>  

</configuration>  