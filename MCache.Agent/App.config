<?xml version="1.0"?>
<configuration>
  <configSections>
     <section name="Netlog" type="Nistec.Logging.NetlogConfig, Nistec.Logger, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
     <section name="MCache" type="Nistec.Caching.Config.CacheConfigServer, Nistec.Cache, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
     <section name="RemoteCache" type="Nistec.Caching.Config.CacheConfigClient, Nistec.Cache, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
  </configSections>

  <Netlog>
    <NetlogSettings>
      <add key="LogFilename" value="C:\Dev\Logs\Cache\Cache.log"/>
      <add key="LogLevel" value="Debug|Info|Warn|Error|Trace"/>
      <add key="LogMode" value="File|Console"/>
    </NetlogSettings>
  </Netlog>

  <RemoteCache>

    <CacheApiSettings>
      <add key="RemoteBundleHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteCacheHostName" value = "nistec_cache"/>
      <add key="RemoteSyncCacheHostName" value = "nistec_cache_sync"/>
      <add key="RemoteSessionHostName" value = "nistec_cache_session"/>
      <add key="RemoteDataCacheHostName" value = "nistec_cache_data"/>

      <add key="RemoteCacheManagerHostName" value = "nistec_cache_manager"/>

      <add key="IsRemoteAsync" value = "false"/>
      <add key="EnableRemoteException" value = "false"/>

    </CacheApiSettings>
  </RemoteCache>
    
    <MCache>

    <CacheApiSettings>
     
      <add key="RemoteBundleHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteCacheHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteSyncCacheHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteSessionHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteDataCacheHostName" value = "nistec_cache_bundle"/>
      <add key="RemoteCacheManagerHostName" value = "nistec_cache_manager"/>
           
      <add key="IsRemoteAsync" value = "false"/>
      <add key="EnableRemoteException" value = "false"/>
     
   </CacheApiSettings>
    
    <CacheSettings>
      <!-- Performance -->
      <add key="EnableAsyncTask" value="true"/>
      <add key="EnableSizeHandler" value="false"/>
      <add key="EnablePerformanceCounter" value="false"/>
      <add key="MaxSize" value="2000000"/>
      <add key="DefaultExpiration" value="30"/>
      <add key="EnableLog" value="true"/>
      <add key="EnableSyncTypeEventTrigger" value="false"/>
      <add key="AutoResetIntervalHours" value="12"/>

      

      <!-- Session -->
      <add key="SessionTimeout" value="0"/>
      <add key="MaxSessionTimeout" value="1440"/>

      <!-- sync Interval in seconds-->
      <add key="SyncInterval" value="60"/>
      <add key="SyncBoxInterval" value="60"/>
      <add key="SyncConfigFile" value = "C:\Dev\Nistec\Git_4.0.2.0\MCache\References\Config\mcSyncCache.xsync"/>
      <add key="EnableSyncFileWatcher" value = "true"/>
      <add key="ReloadSyncOnChange" value = "true"/>
      <add key="SyncTaskerTimeout" value="60"/>
      <add key="RemoveExpiredItemOnSync" value="true"/>
      <add key="DbConfigFile" value = ""/>

      <!-- Protocol NA|Tcp|Pipe|Http-->
      <add key="RemoteCacheProtocol" value = "NA"/>
      <add key="SyncCacheProtocol" value = "Pipe"/>
      <add key="SessionCacheProtocol" value = "NA"/>
      <add key="DataCacheProtocol" value = "NA"/>
      <add key="CacheManagerProtocol" value = "Pipe"/>
      <!--Bundle-->
      <add key="EnableTcpBundle" value = "true"/>
      <add key="EnablePipeBundle" value = "true"/>
      <add key="EnableHttpBundle" value = "false"/>
      
    </CacheSettings>

    <TcpServerSettings>

        <host HostName="nistec_cache_bundle"
           Address="localhost"
           Port="13000"
           IsAsync="false"
           SendTimeout="500000"
           ProcessTimeout="0"
           ReadTimeout="100000"
           ReceiveBufferSize="4096"
           SendBufferSize="4096"
           MaxSocketError="50"
           MaxServerConnections="4"/>

        <host HostName="nistec_cache"
           Address="localhost"
           Port="13001"
           IsAsync="false"
           SendTimeout="5000"
           ProcessTimeout="0"
           ReadTimeout="10000"
           ReceiveBufferSize="4096"
           SendBufferSize="4096"
           MaxSocketError="50"
           MaxServerConnections="1"/>

        <host HostName="nistec_cache_data"
           Address="localhost"
           Port="13002"
           IsAsync="false"
           SendTimeout="5000"
           ProcessTimeout="0"
           ReadTimeout="1000"
           ReceiveBufferSize="4096"
           SendBufferSize="4096"
           MaxSocketError="50"
           MaxServerConnections="1"/>

        <host HostName="nistec_cache_session"
           Address="localhost"
           Port="13003"
           IsAsync="false"
           SendTimeout="5000"
           ProcessTimeout="0"
           ReadTimeout="1000"
           ReceiveBufferSize="4096"
           SendBufferSize="4096"
           MaxSocketError="50"
          MaxServerConnections="1"/>

        <host HostName="nistec_cache_sync"
