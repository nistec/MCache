          MaxAllowedServerInstances="255"/>

      <host HostName="nistec_cache_sync"
         PipeName="nistec_cache_sync"
         PipeDirection="InOut"
         PipeOptions="None"
         VerifyPipe="nistec_cache"
         ConnectTimeout="5000"
         InBufferSize="8192"
         OutBufferSize="8192"
         MaxServerConnections="1"
         MaxAllowedServerInstances="255"/>

      </PipeServerSettings>

    <HttpServerSettings>

        <host HostName="nistec_cache_bundle"
           Address="http://localhost:13100/"
           Method="post"
           SendTimeout="5000"
           ProcessTimeout="0"
           ReadTimeout="10000"
           MaxErrors="50"
           MaxServerConnections="1"/>

        <host HostName="nistec_cache"
           Address="http://localhost:13101/"
           Method="post"
